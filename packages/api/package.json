{
  "name": "@oasis-sh/api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "watch": "concurrently \"tsc --w\" \"tsc-alias -w\"",
    "build": "tsc && tsc-alias && node dist/build.js",
    "bcg": "tsc && tsc-alias",
    "dev:start": "nodemon dist/index.js --ignore src",
    "dev": "concurrently \"tsc -w\" \"yarn run-app\"",
    "run-app": "cd dist && nodemon -e \"js\" --exec \"cd .. && tsc-alias && node dist/index.js\"",
    "typeorm:cli": "ts-node ../../node_modules/typeorm/cli -f ./src/ormconfig.ts",
    "typeorm:generate_migration": "yarn typeorm:cli migration:generate -d ./src/migrations -n",
    "typeorm:run_migrations": "yarn typeorm:cli migration:run",
    "db:setup": "node dist/test-utils/setup.js",
    "test": "jest --silent -c ./dist/jest.config.js",
    "clean": "rimraf dist",
    "prettify": "prettier --write src/{.,entities,modules,lib,utils}/*.ts",
    "lint": "yarn run eslint . --ext ts --ext tsx --ext js"
  },
  "prettier": "@oasis-sh/prettier-config",
  "eslintConfig": {
    "extends": "@oasis-sh/eslint-config"
  },
  "devDependencies": {
    "@types/concurrently": "^6.2.0",
    "@types/connect-redis": "^0.0.16",
    "@types/dotenv": "^8.2.0",
    "@types/eslint": "^7.2.13",
    "@types/express": "^4.17.12",
    "@types/express-session": "^1.17.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/passport": "^1.0.6",
    "@types/passport-discord": "^0.1.3",
    "@types/passport-github2": "^1.2.4",
    "@types/passport-google-oauth20": "^2.0.8",
    "@types/passport-twitter": "^1.0.36",
    "@types/prettier": "^2.2.3",
    "@types/rimraf": "^3.0.0",
    "@types/spotify-web-api-node": "^5.0.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "chalk": "^4.1.1",
    "concurrently": "^6.2.0",
    "cross-fetch": "^3.1.4",
    "eslint": "^7.27.0",
    "jest": "^27.0.4",
    "nanoid": "^3.1.23",
    "prettier": "^2.3.0",
    "sqlite3": "^5.0.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "tsc-alias": "^1.2.11",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "apollo-server-express": "^2.25.0",
    "class-validator": "^0.13.1",
    "connect-redis": "^6.0.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "graphql-query-complexity": "^0.8.1",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-custom": "^1.1.1",
    "passport-discord": "^0.1.4",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "pg": "^8.6.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "spotify-web-api-node": "^5.0.2",
    "tslib": "^2.2.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.33",
    "uuid": "^8.3.2"
  }
}
