    "dotenv": "^9.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "passport": "^0.4.1",
    "passport-discord": "^0.1.4",
    "passport-github2": "^0.1.12",
    "pg": "^8.6.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.32",
    "uuid": "^8.3.2"
  }
}
