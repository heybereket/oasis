{
  "name": "@oasis/api",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "start": "node .",
    "watch": "tsc --watch",
    "build": "tsc && NODE_ENV=development node dist/build.js",
    "dev:start": "nodemon dist/build.js --ignore src",
    "dev": "concurrently \"yarn:watch\" \"yarn:dev:start\"",
    "typeorm:cli": "ts-node ../../node_modules/typeorm/cli -f ./src/ormconfig.ts",
    "typeorm:generate_migration": "yarn typeorm:cli migration:generate -d ./src/migrations -n",
    "typeorm:run_migrations": "yarn typeorm:cli migration:run",
    "db:setup": "node dist/test-utils/setup.js",
    "test": "yarn db:setup && jest",
    "clean": "rimraf dist/*"
  },
  "keywords": [],
  "author": "oasis.sh",
  "license": "MIT",
  "devDependencies": {
    "@types/concurrently": "^6.0.1",
    "@types/connect-redis": "^0.0.16",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4",
    "@types/express-session": "^1",
    "@types/passport": "^1.0.6",
    "@types/passport-discord": "^0",
    "@types/passport-github2": "^1.2.4",
    "@types/uuid": "^8.3.0",
    "concurrently": "^6.0.2",
    "jest": "^26.6.3",
    "sqlite3": "^5.0.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "apollo-server-express": "^2.24.0",
    "class-validator": "^0.13.1",
