{
  "compilerOptions": {
    "jsx": "react",
    "moduleResolution": "node",
    "target": "es2017",
    "importsNotUsedAsValues": "error",
    "isolatedModules": true,
    "sourceMap": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["DOM"]
  }
}
