{
  "name": "@oasis-sh/sdk",
  "version": "0.0.0",
  "description": "The official wrapper for the Oasis API.",
  "main": "dist/index.js",
  "keywords": [
    "oasis",
    "bot"
  ],
  "module": "dist/index.js",
  "scripts": {
    "publish": "npm publish",
    "gen-client": "ts-node src/client",
    "build": "graphql-codegen && ts-node src/client && tsc",
    "watch": "tsc --watch",
    "gen": "graphql-codegen",
    "watch:gen": "graphql-codegen -w",
    "watch:tsc": "tsc --watch",
    "dev": "run-p \"watch:gen\" \"watch:tsc\"",
    "clean": "rimraf dist/*"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Bereket Semagn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oasis-sh/oasis.git"
  },
  "bugs": {
    "url": "https://github.com/oasis-sh/oasis/issues"
  },
  "homepage": "https://github.com/oasis-sh/oasis#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/typescript": "^1.22.4",
    "@types/jest": "^26.0.23",
    "@types/node": "^17.0.1",
    "dotenv": "^10.0.0",
    "jest": "^27.0.5",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "cross-fetch": "^3.1.4",
    "ee-ts": "^1.0.2",
    "node-fetch": "^2.6.1"
  }
}
