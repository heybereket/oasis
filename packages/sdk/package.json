{
  "name": "@oasis-sh/sdk",
  "version": "0.0.0",
  "description": "The official wrapper for the Oasis API.",
  "main": "src/index.ts",
  "keywords": [
    "oasis",
    "bot"
  ],
  "scripts": {
    "publish": "npm publish",
    "build": "graphql-codegen && ts-node src/client && tsc",
    "watch": "tsc --watch",
    "gen": "graphql-codegen",
    "watch:gen": "graphql-codegen -w",
    "watch:tsc": "tsc --watch",
    "dev": "run-p \"watch:gen\" \"watch:tsc\"",
    "clean": "rimraf dist/*"
  },
  "prettier": "@oasis-sh/prettier-config",
  "eslintConfig": {
    "extends": "@oasis-sh/eslint-config"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Bereket Semagn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oasis-sh/oasis.git"
  },
  "bugs": {
    "url": "https://github.com/oasis-sh/oasis/issues"
  },
  "homepage": "https://github.com/oasis-sh/oasis#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/typescript": "^1.22.1",
    "@types/eslint": "^7.2.13",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.2",
    "@types/node-fetch": "^2.5.10",
    "@types/prettier": "^2.3.0",
    "dotenv": "^10.0.0",
    "jest": "^27.0.4",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "4.3.3"
  },
  "dependencies": {
    "ee-ts": "^1.0.2",
    "eslint": "^7.28.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.3.1"
  }
}
