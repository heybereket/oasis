{
  "name": "@oasis-sh/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsc -p server && node server/dist/index.js",
    "build": "next build && tsc -p server",
    "start": "NODE_ENV=production node server/dist/index.js",
    "prettify": "prettier --write src/{.,components,modules,pages,lib}/*.tsx src/{components,modules,pages,lib}/**/*.tsx",
    "lint": "yarn run eslint . --ext ts --ext tsx --ext js",
    "clean": "rimraf .next/*",
    "setup:remote": "yarn clean || echo \"API_MODE='remote'\\\nNEXT_PUBLIC_BASE_URL='http://localhost:3000'\" > .env",
    "cy:install": "cypress install",
    "cy:run": "cypress run --browser chrome",
    "cy:open": "cypress open --browser chrome",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^11.0.0",
    "@sentry/react": "^6.7.2",
    "@sentry/tracing": "^6.7.2",
    "express": "^4.17.1",
    "formik": "^2.2.9",
    "github-markdown-css": "^4.0.0",
    "next": "^11.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-infinite-scroll-component": "^6.1.0",
    "react-markdown": "^6.0.2",
    "react-syntax-highlighter": "^15.4.3",
    "remark-gfm": "^1.0.0",
    "tailwindcss": "^2.2.4",
    "typescript": "4.3.4",
    "zustand": "^3.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/eslint-parser": "^7.14.7",
    "@cypress/webpack-preprocessor": "^5.9.0",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.3.0",
    "@storybook/addon-essentials": "^6.3.0",
    "@storybook/addon-links": "^6.3.0",
    "@storybook/react": "^6.3.0",
    "@tailwindcss/typography": "^0.4.1",
    "@types/express": "^4.17.12",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "^15.12.4",
    "@types/node-fetch": "^2.5.10",
    "@types/react-dom": "^17.0.8",
    "@types/react-syntax-highlighter": "^13.5.1",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.1",
    "cypress": "^7.6.0",
    "eslint": "^7.29.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "next-transpile-modules": "^8.0.0",
    "node-fetch": "^2.6.1",
    "postcss": "^8.3.5",
    "prettier": "2.3.2",
    "pretty-ms": "^7.0.1",
    "rimraf": "^3.0.2",
    "tailwind-scrollbar": "^1.3.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "webpack": "^5.40.0"
  }
}
